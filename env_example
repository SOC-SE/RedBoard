# NMAP Dashboard Configuration
# Copy this file to .env and update the values

# Server settings
PORT=8080
GIN_MODE=release

# Security - CHANGE THESE IN PRODUCTION!
# Session secret (generate with: openssl rand -base64 32)
SESSION_SECRET=change-me-to-a-random-32-character-string

# Default admin password (used on first run)
ADMIN_PASSWORD=changeme

# Database path (SQLite)
# Default is ./dashboard.db in current directory
DB_PATH=./dashboard.db

# API Base URL (usually leave empty unless behind reverse proxy)
API_BASE_URL=
